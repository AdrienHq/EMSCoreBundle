{% trans_default_domain 'EMSCoreBundle' %}
<script type="text/javascript">
<!--

//-->

function prepareForm(value){
	$('.analyzer_option').closest('.form-group').hide();
	const options = $('#analyzer_options_type').data('fields-to-display-by-value')
    options[value].forEach(function(element) {
        $('#analyzer_options_'+element).closest('.form-group').show();
    });
}


$(window).ready(function() {
	$('#analyzer_options_type').change(function(eventObject){
		prepareForm($(this).val());
	});
	prepareForm($('#analyzer_options_type').val())
});


</script>